{
  "name": "@meister/reroute",
  "version": "1.3.2",
  "description": "HTTP Redirect service",
  "keywords": [
    "http",
    "nodejs",
    "server",
    "redirect"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meister/reroute.git"
  },
  "main": "./server/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "test": "jest",
    "lint": "eslint .",
    "version": "changelog-updater && git add CHANGELOG.md"
  },
  "author": "Martin Kapp <martin@coderats.net>",
  "license": "MIT",
  "devDependencies": {
    "changelog-updater": "^2.0.3",
    "eslint": "^8.56.0",
    "eslint-config-pipedrive": "13.0.4",
    "eslint-plugin-jest": "^27.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "cli-color": "^2.0.3",
    "cli-color-tty": "^2.0.0",
    "lodash.at": "^4.6.0"
  },
  "overrides": {
    "cli-color-tty": {
      "cli-color": "$cli-color"
    }
  },
  "jest": {
    "rootDir": ".",
    "testEnvironment": "node",
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "/tests/.*\\.test\\.js$",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.{js}",
      "<rootDir>/server/**/*.{js}"
    ]
  }
}
